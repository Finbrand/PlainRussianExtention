{
  "name": "Sofi Plane Russian plugin",
  "description": "Bar for browsing pages",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
	},
  "browser_action": {
    "default_popup": "popup.html",
	"default_icon": {
      "19": "img/PlainRussian19.png",
      "38": "img/PlainRussian38.png"
    } 
  },
  "permissions": [
   "activeTab",
   "tabs",
   "http://*.plainrussian.ru/",
   "https://*.plainrussian.ru/"
   ]
}